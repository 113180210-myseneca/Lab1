#A function that takes an input from the user and calculate someone’s age

def age():
    try:
        user_year=int(input("Enter your birth year"))
        curr_year=int(input("Enter the current year"))
        usrage=curr_year-user_year
        return usrage
    except ValueError:
        print("Please enter an integer.")    

def helloWorld():
    print(‘Hello World’)
    
helloWorld()